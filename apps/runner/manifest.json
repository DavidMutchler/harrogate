{
  "name": "Runner",
  "hidden": false,
  "fonticon": "fa-rocket",
  "exec": "exec.coffee",
  "description": "Runs a user program",
  "category": "System",
  "angular_ctrl": "resources/runner-view-controller.coffee",
  "web_api": {
    "run": {
      "uri": "/api/run",
      "description": "Start, stop and get information about running robot programs"
    }
  },
  "event_groups": {
    "runner_events": {
      "namespace": "/runner",
      "events": {
        "stdout": {
          "id": "stdout",
          "publisher": "SERVER",
          "description": "stdout sent from the program."
        },
        "stderr": {
          "id": "stderr",
          "publisher": "SERVER",
          "description": "stderr sent from the program."
        }
      }
    }
  }
}