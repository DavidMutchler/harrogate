script(src='/shared/client/scripts/harrogate-navbar-module.coffee')

nav.navbar.navbar-default(role='navigation', data-ng-controller='HarrogateNavbarCtrl')
  .container-fluid
    .navbar-header
      button.navbar-toggle.collapsed(data-toggle='collapse', data-target='#harrogate-navbar-collapse')
        i.fa.fa-bars
      .navbar-brand
        img(src='/shared/client/images/botguy-logo.png')
    #harrogate-navbar-collapse.collapse.navbar-collapse
      ul.nav.navbar-nav
          // Home
          li(data-ng-class="{active: $location.path() == '/apps/home'}")
            a(href='#/apps/home')
              i.fa.fa-home.fa-fw.fa-2x
